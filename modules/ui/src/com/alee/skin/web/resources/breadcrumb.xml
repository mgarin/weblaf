<skin>

    <!-- com.alee.extended.breadcrumb.WebBreadcrumb -->

    <!-- Content panel -->
    <style type="panel" id="breadcrumb">
        <painter>
            <undecorated>false</undecorated>
            <paintFocus>true</paintFocus>
        </painter>
    </style>

    <!-- Label element -->
    <style type="label" id="breadcrumb-label" padding="1,5,1,5">
        <painter class="WebBreadcrumbLabelPainter" />
    </style>

    <!-- Button element -->
    <style type="button" id="breadcrumb-button" padding="2,5,2,5">
        <painter class="WebBreadcrumbButtonPainter" />
    </style>

    <!-- Toggle button element -->
    <style type="togglebutton" id="breadcrumb-togglebutton" padding="2,5,2,5">
        <painter class="WebBreadcrumbToggleButtonPainter" />
    </style>

    <!-- Panel element -->
    <style type="panel" id="breadcrumb-panel" padding="1,5,1,5">
        <painter class="WebBreadcrumbPanelPainter" />
    </style>

</skin>